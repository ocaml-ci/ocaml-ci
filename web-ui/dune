(rule
 (target static.ml)
 (deps
  (source_tree static))
 (action
  (run ocaml-crunch --mode=plain --output=%{target} static)))

(executable
 (name main)
 (public_name ocaml-ci-web)
 (package ocaml-ci-web)
 (libraries
  ansi
  dream
  logs.cli
  prometheus-app.unix
  ocaml-ci-api
  capnp-rpc-unix
  ocaml-ci-web.controller
  ocaml-ci-web.view))
